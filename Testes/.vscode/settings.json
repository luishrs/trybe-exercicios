"eslint.alwaysShowStatusbarStatus": "off",
"eslint.autoFixOnSave": true,
"eslint.options": {
  "extensions": [".js", ".jsx", ".ts", ".tsx"]
},
"eslint.validate": [
  "javascript",
  "javascriptreact",
  "typescript",
  "typescriptreact"
],
"editor.codeActionsOnSave": {
  "source.fixAll.eslint": true
},
"editor.renderControlCharacters": true,
"editor.renderWhitespace": "all",
"editor.rulers": [
  80
],
"editor.tabSize": 2,
"editor.wordWrap": "on"
